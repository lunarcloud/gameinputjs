{
    "name": "gameinput",
    "version": "10.0.0",
    "description": "",
    "private": true,
    "scripts": {
        "lint-fix": "npx eslint --ignore-path .gitignore --ext .js . --fix",
        "lint": "npx eslint --ignore-path .gitignore --ext .js .",
        "pre-build": "shx rm -rf dist && shx mkdir dist && shx cp  src/tester.html dist/ && shx cp -r src/css dist/ && shx cp -r src/img dist/",
        "build": "npm run pre-build && npx webpack --config webpack.config.js --mode development",
        "build-prod": "npm run pre-build && npx webpack --config webpack.config.js --mode production",
        "serve": "npx serve",
        "docs": "shx rm -rf docs &&npx jsdoc -c jsdoc.conf.json"
    },
    "devDependencies": {
        "eslint": "^8.49.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-editorconfig": "^4.0.3",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsdoc": "^46.8.1",
        "eslint-plugin-n": "^16.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "jsdoc": "^4.0.2",
        "jsdoc-tsimport-plugin": "^1.0.5",
        "serve": "^14.2.1",
        "shx": "^0.3.4",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "license": "MIT",
    "author": "Samuel Sarette"
}
