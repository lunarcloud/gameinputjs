{
    "name": "gameinput",
    "version": "10.0.0",
    "description": "",
    "private": true,
    "scripts": {
        "lint-fix": "npx eslint --ignore-path .gitignore --ext .js . --fix",
        "lint": "npx eslint --ignore-path .gitignore --ext .js .",
        "pre-build": "rm -r dist && mkdir dist && cp src/tester.html dist/ && cp -r src/css dist/ && cp -r src/img dist/",
        "build": "npm pre-build && npx webpack --config webpack.config.js --mode development",
        "build-prod": "npm run pre-build && npx webpack --config webpack.config.js --mode production",
        "serve": "npx serve"
    },
    "devDependencies": {
        "eslint": "^8.49.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-editorconfig": "^4.0.3",
        "eslint-plugin-import": "^2.28.1",
        "eslint-plugin-jsdoc": "^46.8.1",
        "eslint-plugin-n": "^16.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "serve": "^14.2.1",
        "webpack": "^5.88.2",
        "webpack-cli": "^5.1.4"
    },
    "license": "MIT",
    "author": "Samuel Sarette"
}
