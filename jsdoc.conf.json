{
    "plugins": [
        "plugins/markdown",
        "plugins/summarize",
        "node_modules/jsdoc-tsimport-plugin/index.js"
    ],
    "recurseDepth": 10,
    "source": {
        "includePattern": ".+\\.js(doc)?$",
        "include": ["src"],
        "exclude": ["node_modules", "dist", "css"]
    },
    "sourceType": "module",
    "opts": {
        "encoding": "utf8",
        "destination": "./dist/docs/",
        "recurse": true
    },
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc"]
    }
}